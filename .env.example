# Example environment variables for Mobile Verification Messaging App

# Enable or disable admin panel access (true/false)
REACT_NATIVE_APP_ENABLE_ADMIN=true

# OTP length used in the app
REACT_NATIVE_APP_OTP_LENGTH=6

# API key for any external SMS service (if integrated)
# Leave blank if not used
REACT_NATIVE_APP_SMS_API_KEY=

# Base URL for any backend service (if applicable)
REACT_NATIVE_APP_API_BASE_URL=

# Expo environment: development or production
REACT_NATIVE_APP_ENV=development
